{
    {% if consul_is_webui %}
    "ui_dir": "{{consul_webui_install_dir}}/webui",
    {% endif %}
    "start_join": [{% for host in consul_servers %}"{{host}}"{% if not loop.last %},{% endif %}{% endfor %}],
    "domain": "{{consul_domain}}",
    "data_dir": "{{consul_data_dir}}",
    "node_name": "{{consul_node_name}}",
    "server": {{ "true" if consul_is_server else "false" }},
    {% if consul_is_server %}
    "bootstrap_expect": {{consul_servers|length}},
    {% endif %}
    "ports": {
        "dns": {{ consul_port_dns }},
        "http": {{ consul_port_http }},
        "https": {{ consul_port_https }},
        "rpc": {{ consul_port_rpc }},
        "serf_lan": {{ consul_port_serf_lan }},
        "serf_wan": {{ consul_port_serf_wan }},
        "server": {{ consul_port_server }}
    },
    "rejoin_after_leave": true,
    "leave_on_terminate": true
}